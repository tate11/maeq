<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!--Ventana emergente reporte de Panel de control-->
        <record id="eliterp_view_form_report_control_panel_wizard" model="ir.ui.view">
            <field name="name">eliterp_view_form_report_control_panel_wizard</field>
            <field name="model">eliterp.report.control.panel.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group col="4">
                        <field name="start_date"/>
                        <field name="end_date"/>
                        <field name="state"/>
                    </group>
                    <footer>
                        <!--TODO
                        <button name="print_report_xlsx"
                                type="object"
                                class="oe_highlight"
                                string="XLSX"/>
                        -->
                        <button name="print_report_pdf"
                                type="object"
                                class="oe_highlight"
                                string="PDF"/>
                        <button string="Descartar" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>